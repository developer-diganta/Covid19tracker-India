STATE:<%=state%>
<div class="container">
  <table class="table table-hover table-bordered tab center" id="myTable" style="border-color:black; width:80%; ">
    <thead>
      <tr>
        <th scope="col" onclick="sortTable(0)" style="width:40%" class="the">District<i class="fas fa-angle-up"></i>

</th>
        <th scope="col" onclick="sortTableNum()">Confirmed</th>
        <th scope="col" onclick="sortTableNum()">Active</th>
        <th scope="col" onclick="sortTableNum()">Recovered</th>
        <th scope="col" onclick="sortTableNum()">Death</th>
        <!-- <th scope="col">Last Updated Time</th> -->
      </tr>
    </thead>





  <%  for(var key in found){%>
      <tr>
        <td > <%=key%></td>

      <td style="background-color:#5FAEFF;" class="confirmed"><%=found[key].confirmed%></td>
      <td style="background-color:#FAFF50" class="active"><%=found[key].active%></td>
      <td  style="background-color:#74FF6F" class="recovered"><%=found[key].recovered%></td>
      <td style="background-color:#F83C3C;" class="death"><%=found[key].deceased%></td>
      <!-- <td style="background-color:grey;" class="dat"><%=found[key].lastupdatedtime%></td> -->

</tr>
<%}%>
</table>
</div>
<cite>
