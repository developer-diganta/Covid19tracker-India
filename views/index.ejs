  <div class="sr">

  <span style="font-size:10px;">Search for your State</span>
  <form style="padding:3px;margin-bottom:3%;" class="fo">
    <!-- <button class="btn " class="bt" > <i class="fas fa-search"></i> </button> -->
    <div class="input-group flex-nowrap">
  <span class="input-group-text" id="addon-wrapping" style="background:none;border-right:none;"><i class="fas fa-search" style="font-size:32px;"></i></span>
  <input type="text" class="form-control me-2"  placeholder="eg: Delhi" aria-label="Search" id="myInput" onkeyup="myFunction()" style="border-left:none;" >
</div>
       <!-- <input class="form-control me-2 textBox" type="search" placeholder="Search"  aria-label="Search"> -->
     </form>
   </div>
   <div class="table-responsive">


  <table class="table table-hover table-bordered tab center" id="myTable" style="border-color:black; width:80%;">
    <thead>
      <tr>
        <th scope="col" onclick="sortTable(0)" style="width:40%" class="the state">State   <i class="fas fa-sort-up"></i>

</th>
        <th scope="col" onclick="sortTableNum()">Confirmed</th>
        <th scope="col" onclick="sortTableNum()">Active</th>
        <th scope="col" onclick="sortTableNum()">Recovered</th>
        <th scope="col" onclick="sortTableNum()">Death</th>
        <th scope="col">Last Updated Time</th>
      </tr>
    </thead>
    <% for(i=1;i<found.length;i++){
      %>
      <tr>
        <td >
          <form class="" action="/state" method="post">

          <button class="st" type="submit" name="state" value="<%=found[i].state%>"><%=found[i].state%>
          <%if(found[i].statenotes){%>
          <span data-toggle="tooltip"  title="<%=found[i].statenotes%>"><i class="fas fa-circle" style="font-size:8px;color:blue;"></i>  </span><%}%>
        </button>
            </form>

        </td>
        <td style="background-color:#5FAEFF;" class="confirmed"><%=found[i].confirmed%>
                <%if(found[i].deltaconfirmed!=0){
                  %>
                   (<span style="color:red;"><i class="fas fa-arrow-up"></i><%=found[i].deltaconfirmed%></span>)
                <%}%>
        </td>
        <td style="background-color:#FAFF50" class="active"><%=found[i].active%></td>
        <td  style="background-color:#74FF6F" class="recovered"><%=found[i].recovered%>
          <%if(found[i].deltarecovered!=0){
            %>
             (<span style="color:green;"><i class="fas fa-arrow-up"></i><%=found[i].deltarecovered%></span>)
          <%}%>
        </td>
        <td style="background-color:#F83C3C;" class="death"><%=found[i].deaths%>
          <%if(found[i].deltadeaths!=0){
            %>
             (<span style="color:maroon;"><i class="fas fa-arrow-up"></i><%=found[i].deltadeaths%></span>)
          <%}%>
        </td>
        <td style="background-color:grey;" class="dat"><%=found[i].lastupdatedtime%></td>
      </tr>
    <%}%>
  </table>
 </div>



<div id="hidden" style="display:none">
  <%=date%>
</div>


